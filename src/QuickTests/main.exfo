extern "C" func malloc(int): char_ptr;
extern "C" func scanf(char_ptr, ...): void;
extern "C" func printf(char_ptr, ...): void;
extern "C" func free(char_ptr): void;

func main(): void {
    name := malloc(35);
    printf("Whats your name?: "); 
    scanf("%34s", name);

    printf("Hello, %s! %c", name, 10);
    free(name);
}

