extern "C" func printf(char_ptr, ...): void;

func print_i64(a: int): void {
    printf("%d%c", a, 10);
}


func print_bool(b: bool): void {
    if b printf("true");
    else printf("false");
    printf("%c", 10);
}


func main(): int {
    a := 1;
    print_i64(a);
    a := a + 1; // Shadowing
    print_i64(a);

    a := false; // Changing type
    print_bool(a);

    a := 2;
    a = a + 1; // Mutation
    print_i64(a);

    return 0;
}
