extern "C" func puts(char_ptr): void; 
func print_bool(a: bool): void {
    if a puts("true");
    else puts("false");
}

func main(): void { 
   a := 15; 
   b := 15; 
   print_bool(a > b); 
   if a > b 
       puts("a is greater than b"); 
   else if a < b 
       puts("a is less than b"); 
   else puts("a is equal to b");
}