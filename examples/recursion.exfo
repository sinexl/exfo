extern "C" func printf(char_ptr, ...): void;

func factorial(n: int): int {
    if n < 1 return 1;
    return n * factorial(n - 1);
}

func fibonacci(n: int): int {
    if n == 0 return 0;
    if n == 1 return 1;

    return fibonacci(n - 1) + fibonacci(n - 2);
}

func main(): void { 
    printf("10!     = %d\n", factorial(10));
    printf("fib(10) = %d\n", fibonacci(10));
}
