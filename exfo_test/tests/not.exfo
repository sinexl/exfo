extern "C" func printf(char_ptr, ...): void;

func get_string(a: bool): char_ptr {
    if a return "true";
    return "false";
}

func main(): int {
    a := true;
    printf("true  = %5s, !true  = %5s, !!true  = %5s\n", get_string(a), get_string(!a), get_string(!!a));
    a = !a;
    printf("false = %5s, !false = %5s, !!false = %5s\n", get_string(a), get_string(!a), get_string(!!a));
    return 0;
}
